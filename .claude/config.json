{
  "changelog-plugin": {
    "model": "inherit",
    "changelog": {
      "outputPath": "./changelog.md",
      "followUp": {
        "decisions": "ask",
        "whattotest": "ask"
      }
    },
    "decisions": {
      "outputPath": "./decisions.md"
    },
    "whattotest": {
      "enabled": "auto",
      "outputPath": "./TestFlight/WhatToTest.txt",
      "onExisting": "ask"
    },
    "release": {
      "gitMode": "auto"
    }
  }
}
